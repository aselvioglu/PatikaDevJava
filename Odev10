import java.util.Scanner;

public class Odev10 {

	public static void main(String[] args) {
		//Sıcaklık düzeyine göre etkinlik önerir
		Scanner sc=new Scanner(System.in);
		System.out.println("Sıcaklık düzeyini giriniz:");
		double sicaklik=sc.nextDouble();
		
		if(sicaklik<5) {System.out.println("Kayak yapabilirsiniz.");}
		else if(sicaklik>=5&&sicaklik<=10) {System.out.println("Sinemaya gidebilirsiniz.");}
		else if (sicaklik >10&&sicaklik<=15) {System.out.println("Sinemaya gidebilirsiniz ya da \npiknik yapabilirsiniz.");}
		else if(sicaklik>15 && sicaklik<=25) {System.out.println("Piknik yapabilirsiniz.");}
		else {System.out.println("Yüzebilirsiniz.");}
		
		

	}

}
